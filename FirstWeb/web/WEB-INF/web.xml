    <jsp-file>/FirstJSP.jsp</jsp-file>
</servlet>
    <servlet-mapping>
        <servlet-name>firstjsp</servlet-name>
        <url-pattern>/jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Directive</servlet-name>
        <jsp-file>/Dir</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Directive</servlet-name>
        <url-pattern>/Dir</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Error</servlet-name>
        <jsp-file>/errorPage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Error</servlet-name>
        <url-pattern>/errorpage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>isError</servlet-name>
        <jsp-file>/isErrorPage</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>isError</servlet-name>
        <url-pattern>/isErrorPage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>title</servlet-name>
        <jsp-file>/title.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>title</servlet-name>
        <url-pattern>/title</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>include</servlet-name>
        <jsp-file>/include.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>include</servlet-name>
        <url-pattern>/include</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Jspinclude</servlet-name>
        <jsp-file>/Include/Jspinclude.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Jspinclude</servlet-name>
        <url-pattern>/Include/Jspinclude</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>calculate</servlet-name>
        <jsp-file>/Include/calculate.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>calculate</servlet-name>
        <url-pattern>/Include/calculate</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>multi</servlet-name>
        <jsp-file>/Include/multi.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>multi</servlet-name>
        <url-pattern>/Include/multi</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>div</servlet-name>
        <jsp-file>/Include/div.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>div</servlet-name>
        <url-pattern>/Include/div</url-pattern>
    </servlet-mapping>
</web-app>